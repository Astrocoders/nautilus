{
  "name": "reason-reroute",
  "version": "0.0.4",
  "description": "Tiny wrapper around ReasonReact.Router ",
  "main": "index.js",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "prepublish": "npm run build",
    "run-ios": "react-native run-ios --project-path \"./example/.native/ios\"",
    "run-android": "react-native run-android --root \"./example/.native\"",
    "start-example": "node ./node_modules/react-native/local-cli/cli.js start --root ./example",
    "test": "exit 0"
  },
  "keywords": [
    "router",
    "react",
    "reason",
    "reasonml"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callstack/reroute.git"
  },
  "author": "Mike Grabowski <grabbou@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/callstack/reroute/issues"
  },
  "homepage": "https://github.com/callstack/reroute#readme",
  "devDependencies": {
    "bs-platform": "^3.0.0",
    "bs-react-native": "reasonml-community/bs-react-native#f0795f6",
    "react": "^16.3.0-alpha.1",
    "react-native": "0.54.0",
    "react-native-gesture-handler": "^1.0.0-alpha.41",
    "reason-react": "^0.4.1"
  },
  "peerDependencies": {
    "bs-react-native": "reasonml-community/bs-react-native#f0795f6",
    "react-native-gesture-handler": "*",
    "reason-react": "*"
  }
}