{
  "name": "reason-reroute",
  "version": "0.0.4",
  "description": "Tiny wrapper around ReasonReact.Router ",
  "main": "index.js",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "prepublish": "npm run build",
    "run-example":
      "react-native run-ios --project-path \"./example/.native/ios\"",
    "start-example":
      "node ./node_modules/react-native/local-cli/cli.js start --root ./example",
    "test": "exit 0"
  },
  "keywords": ["router", "react", "reason", "reasonml"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callstack/reroute.git"
  },
  "author": "Mike Grabowski <grabbou@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/callstack/reroute/issues"
  },
  "homepage": "https://github.com/callstack/reroute#readme",
  "devDependencies": {
    "bs-platform": "^2.1.0",
    "reason-react": "^0.3.2",
    "bs-react-native": "reasonml-community/bs-react-native#0b23dd8",
    "react-native": "0.54.0",
    "react": "^16.3.0-alpha.1"
  }
}
