{
  "name": "nautilus",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "refmt": 3,
  "bs-dependencies": [
    "reason-react",
    "bs-react-native/reason-react-native",
    "reason-react-compat",
  ],
  "reason": {
    "react-jsx": 3
  },
  "namespace": true,
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [
    {
      "dir": "src",
      "public": "all",
      "subdirs": true
    }
  ],
  "js-post-build": {
    "cmd": "./node_modules/bs-react-native/git-monorepo-usage-trick"
  }
}
