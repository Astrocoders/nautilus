{
  "name": "reroute-native",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "refmt": 3,
  "bs-dependencies": ["reason-react", "rebolt"],
  "reason": {
    "react-jsx": 2
  },
  "namespace": true,
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "sources": [
    {
      "dir": "src",
      "public": "all",
      "subdirs": true
    },
    {
      "dir": "example",
      "type": "dev"
    }
  ]
}
